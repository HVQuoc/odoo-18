<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <!-- Sequence for Brand -->
  <record id="seq_brand_code" model="ir.sequence">
    <field name="name">Brand Sequence</field>
    <field name="code">sale_man.brand</field>
    <field name="prefix">BR</field>
    <field name="padding">4</field>
    <field name="number_next">1</field>
    <field name="number_increment">1</field>
  </record>

  <!-- Sequence for Category -->
  <record id="seq_category_code" model="ir.sequence">
    <field name="name">Category Sequence</field>
    <field name="code">sale_man.category</field>
    <field name="prefix">CAT</field>
    <field name="padding">4</field>
    <field name="number_next">1</field>
    <field name="number_increment">1</field>
  </record>

  <!-- Sequence for Product -->
  <record id="seq_product_code" model="ir.sequence">
    <field name="name">Product Sequence</field>
    <field name="code">sale_man.product</field>
    <field name="prefix">PRD</field>
    <field name="padding">4</field>
    <field name="number_next">1</field>
    <field name="number_increment">1</field>
  </record>

  <!-- Sequence for Program -->
  <record id="seq_program_code" model="ir.sequence">
    <field name="name">Program Sequence</field>
    <field name="code">sale_man.program</field>
    <field name="prefix">PRG</field>
    <field name="padding">4</field>
    <field name="number_next">1</field>
    <field name="number_increment">1</field>
  </record>

  <!-- Sequence for Order -->
  <record id="seq_program_code" model="ir.sequence">
    <field name="name">Order Sequence</field>
    <field name="code">sale_man.order</field>
    <field name="prefix">ORD</field>
    <field name="padding">4</field>
    <field name="number_next">1</field>
    <field name="number_increment">1</field>
  </record>

  <!-- Sequence for Purchase -->
  <record id="seq_program_code" model="ir.sequence">
    <field name="name">Purchase Sequence</field>
    <field name="code">sale_man.purchase</field>
    <field name="prefix">PUR</field>
    <field name="padding">4</field>
    <field name="number_next">1</field>
    <field name="number_increment">1</field>
  </record>
</odoo>
